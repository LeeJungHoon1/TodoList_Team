<script>
	function showAllTodo(){
		alert("send() 호출 완료");
		// $.ajax(); () 인자정보를 객체타입으로 제공함
		$.ajax({
			type:"get",
			dataType: "json",
			url:"/TodoTeamProject/main",
			success:function(data){
				alert(data);
				console.log(data);
				//$("#result").html(data);
				
			},
			error: function(err){
				alert("요청에 실패했습니다.")

			}
		});
	}
</script>
<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>

<script>showAllTodo()</script>

</body>
</html>