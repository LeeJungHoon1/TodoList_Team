<%@page import="org.json.JSONObject"%>
<%@ page language="java" contentType="text/html; charset=EUC-KR"
	pageEncoding="EUC-KR"%>
<%@ page import="java.util.ArrayList"%>
<%@ page import="TodoBoard.Board"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js"></script>

<script>
	
	function sendAll(){
		
		$.ajax({
			
			type:"get",
			dataType:"json",
			url: "/TodoTeamProject/main",
			data: "id="+id,
			success: function(data){
				
				let str = `	<tr>
									<td>Code</td>
									<td>No</td>
									<td>ID</td>
									<td>Contents</td>
									<td>Date</td>
									<td>Star</td>
									<td>Finished</td>
								</tr>`;
								
				
				for(let i=0; i<data.length; i++){
						
					let item = data[i];
					str += `<tr>
									<td>${item.code
										}</td>
									<td>${item.no}</td>
									<td>${item.id}</td>
									<td>${item.contents}</td>
									<td>${item.date}</td>
									<td>${item.star}</td>
									<td>${item.finished}</td>
								</tr>`
								;
					
				}
					$("#result").append(str);
				
				
			},
			
			error: function(err){
				alert("요청에 실패했습니다");
			}	
			
		});
		
		
	}
	
	
</script>

<!DOCTYPE html>
<html>

<head>
<meta charset="EUC-KR">
<title>Insert title here</title>
</head>
<body>
	<div class="wrap">
		<button onclick="sendAll()">할일 목록 보기</button>
		<table id="result"></table>
	</div>
</body>
</html>